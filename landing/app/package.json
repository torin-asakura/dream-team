    "@atls/next-document-with-opengraph": "^0.0.2",
    "@atls/next-sitemap-generator": "^0.0.3",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "events": "^3.3.0",
    "express": "^4.17.1",
    "graphql": "^15.8.0",
    "next": "^12.2.0",
    "next-compose-plugins": "^2.2.0",
    "react": "^17.0.2",
